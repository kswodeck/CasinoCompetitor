<script>
let dialogSup = document.getElementsByTagName('dialog');
  for (i = 0; i < dialogSup.length; i++) {
    dialogPolyfill.registerDialog(dialogSup[i]);
    dialogSup[i].addEventListener("keydown", (e) => {
      if (e.code == 'Escape') {
        document.getElementById('mid-container').style.display = 'block';
      }
    });
  }
</script>
<script src="\javascripts\common.js" type="text/javascript"></script>
<script src="\javascripts\validation.js" type="text/javascript"></script>
